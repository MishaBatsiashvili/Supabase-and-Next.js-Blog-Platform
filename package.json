{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "graphql-merge-schema": "tsx src/graphql/mergeSchema.ts",
    "graphql-typegen": "npm run graphql-merge-schema && graphql-codegen --config src/graphql/codegen.yml",
    "graphql-typegen-watch": "nodemon -e gql,graphql,tsx,ts --ignore src/graphql/schema/schema.graphql --exec 'npm run graphql-typegen'",
    "generate-schema": "dotenv -e .env.local -- sh -c 'npx supabase gen types --lang=typescript --project-id $PROJECT_REF --schema public > ./src/database.schema.ts --debug'",
    "dev": "concurrently -c \"red,green,blue\" -n \"graphql-ts,supabse-ts,next dev\" \"npm run graphql-typegen-watch\" \"npm run generate-schema\" \"next dev\"",
    "next:dev": "next dev",
    "build": "npm run graphql-typegen && next build",
    "start": "next start",
    "lint": "next lint",
    "algolia:build": "dotenv -e .env.local -- sh -c 'tsx src/lib/build-algolia-search.ts'",
    "type-check": "tsc"
  },
  "dependencies": {
    "@algolia/client-search": "^5.13.0",
    "@apollo/client": "^3.11.5",
    "@apollo/server": "^4.11.0",
    "@as-integrations/next": "^3.1.0",
    "@aws-sdk/client-s3": "^3.682.0",
    "@aws-sdk/s3-request-presigner": "^3.682.0",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-tools/load-files": "^7.0.0",
    "@graphql-tools/merge": "^9.0.8",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@hookform/resolvers": "^3.9.0",
    "@nextui-org/pagination": "^2.0.36",
    "@nextui-org/react": "^2.4.8",
    "@nextui-org/system": "^2.2.6",
    "@nextui-org/theme": "^2.2.11",
    "@radix-ui/react-portal": "^1.1.2",
    "@supabase/ssr": "^0.5.0",
    "@supabase/supabase-js": "^2.45.2",
    "algoliasearch": "^5.13.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "cross-fetch": "^4.0.0",
    "framer-motion": "^11.11.11",
    "graphql": "^16.9.0",
    "instantsearch.css": "^8.5.1",
    "moment": "^2.30.1",
    "next": "14.2.6",
    "nextjs-toploader": "^3.7.15",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-instantsearch": "^7.13.7",
    "react-toastify": "^10.0.5",
    "tsx": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.4"
  }
}
